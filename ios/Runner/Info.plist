<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>VithiMap</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>VithiMap</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<!-- Permissions -->
		<key>NSCameraUsageDescription</key>
		<string>Camera access is required to scan QR codes in the app.</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>We need access to your photo library to select images.</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>We need access to your microphone for video recording.</string>
		<key>NSAppleMusicUsageDescription</key>
		<string>This app requires Apple Music access to function properly.</string>
		<key>NSCalendarsUsageDescription</key>
		<string>This app requires Calendar access to function properly.</string>
		<key>NSContactsUsageDescription</key>
		<string>This app requires contacts access to function properly.</string>
		<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
		<string>We are using Phone GPS location for checking distance between current phone location to GPS device Location. We are not storing Phone location to server or anywhere.</string>
		<key>NSLocationAlwaysUsageDescription</key>
		<string>We are using Phone GPS location for checking distance between current phone location to GPS device Location. We are not storing Phone location to server or anywhere.</string>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>We are using Phone GPS location for checking distance between current phone location to GPS device Location. We are not storing Phone location to server or anywhere.</string>
		<key>NSMotionUsageDescription</key>
		<string>This app requires Motion access to function properly.</string>
		<key>NSSpeechRecognitionUsageDescription</key>
		<string>This app requires Speech Recognition access to function properly.</string>
		<!-- Networking -->
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<true/>
		</dict>
		<!-- Flutter settings -->
		<key>io.flutter.embedded_views_preview</key>
		<true/>
		<key>FlutterDeepLinkingEnabled</key>
		<true/>
		<!-- Firebase -->
		<key>FirebaseMessagingAutoInitEnabled</key>
		<false/>
		<!-- Launch Screens -->
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<!-- Main storyboard (if any) -->
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<!-- Orientation -->
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<!-- Status bar control -->
		<key>UIViewControllerBasedStatusBarAppearance</key>
		<false/>
		<!-- Background modes -->
		<key>UIBackgroundModes</key>
		<array>
			<string>fetch</string>
			<string>location</string>
			<string>remote-notification</string>
		</array>
		<!-- iOS performance flags -->
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<!-- Deep linking scheme -->
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>kpg</string>
				</array>
				<key>CFBundleURLName</key>
				<string>com.acube.gps</string>
			</dict>
		</array>
		<key>UIStatusBarHidden</key>
		<true/>
	</dict>
</plist>
